<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_gaysach">
        <t t-call="web.basic_layout">
            <t t-foreach="docs" t-as="doc">
                <div class="page">
                    <div class="row">
                        <table class="table">
                            <t t-foreach="doc.serial_list" t-as="sr">
                                <tr>
                                    <t t-foreach="2" t-as="num">
                                        <td>
                                            <t t-esc="sr.ma_sach.danh_muc_sach.ma_ts"/>
                                        </td>
                                        <td>
                                            <t t-esc="sr.serial_no"/>
                                        </td>
                                    </t>
                                </tr>
                            </t>
                        </table>
                    </div>
                </div>
            </t>
        </t>
    </template>
</odoo>
